package com.sunbeam;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		String string;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter String-");
		string = sc.next();
		System.out.println("Lenght of String-" +string.length());
	}

}
